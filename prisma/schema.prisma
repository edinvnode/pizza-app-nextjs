generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cake {
  id          Int      @id(map: "Cake_pkey") @default(autoincrement())
  name        String
  price       Float
  image       String
  createdAt   DateTime @default(now())
  description String
}
